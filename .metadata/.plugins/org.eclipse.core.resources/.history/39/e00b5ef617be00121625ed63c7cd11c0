CREATE TABLE "pebedba"."USERINFORMATION" 
   (	"UUID" VARCHAR2(28 BYTE) NOT NULL ENABLE, 
	"ANONYMOUSFLAG" NUMBER(*,0), 
	"DISABLEDFLAG" NUMBER(*,0), 
	"CURRENCYID" VARCHAR2(6 BYTE), 
	"LOCALEID" VARCHAR2(6 BYTE), 
	"LOGIN" VARCHAR2(255 BYTE) NOT NULL ENABLE, 
	"PASSWORD" VARCHAR2(255 BYTE), 
	"PASSWORDDISABLEDFLAG" NUMBER(*,0), 
	"PASSWORDREMINDER" VARCHAR2(255 BYTE), 
	"REMINDEREMAIL" VARCHAR2(128 BYTE), 
	"NAME" VARCHAR2(255 BYTE), 
	"DESCRIPTION" VARCHAR2(255 BYTE), 
	"LASTMODIFIED" DATE, 
	"LAST_MODIFIED_BY_USER" VARCHAR2(50 BYTE), 
	"LAST_MODIFIED_BY_PROCESS" VARCHAR2(50 BYTE) NOT NULL ENABLE, 
	"ACCESSCONTROLGROUP" VARCHAR2(128 BYTE), 
	"ACCESSIPRANGE" VARCHAR2(255 BYTE), 
	"ACCESSINFORMATION" LONG, 
	"SINGLEUSEFLAG" NUMBER, 
	"PASSWORDMODIFIEDDATE" DATE, 
	"CREATIONDATE" DATE, 
	"TITLE" VARCHAR2(255 BYTE), 
	"FIRSTNAME" VARCHAR2(255 BYTE), 
	"LASTNAME" VARCHAR2(255 BYTE), 
	"REFERRER" VARCHAR2(255 BYTE), 
	"ORDERCOUNT" NUMBER, 
	"VIP" NUMBER(*,0) DEFAULT 0, 
	"COMMENTS" VARCHAR2(4000 BYTE), 
	"ENABLED" VARCHAR2(1 BYTE) DEFAULT 'Y' NOT NULL ENABLE, 
	"ACCESSIBILITY_ENABLED" VARCHAR2(1 BYTE) DEFAULT 'N' NOT NULL ENABLE, 
	"LAST_LOGIN" DATE, 
	"LAST_CHECKOUT" DATE, 
	"PIN" VARCHAR2(255 BYTE), 
	"IS_TEST_USER" NUMBER(1,0), 
	 PRIMARY KEY ("UUID")
   ) ;

  CREATE UNIQUE INDEX "pebedba"."USERINFORMATION_IAK1" ON "pebedba"."USERINFORMATION" ("LOGIN");
  CREATE INDEX "pebedba"."USERINFORMATION_IX1" ON "pebedba"."USERINFORMATION" ("ACCESSCONTROLGROUP");
  CREATE INDEX "pebedba"."USERINFORMATION_IIE1" ON "pebedba"."USERINFORMATION" ("BACKOFFICEUSERFLAG");
  CREATE UNIQUE INDEX "pebedba"."SYS_C0029356" ON "pebedba"."USERINFORMATION" ("UUID");
  CREATE INDEX "pebedba"."USERINFORMATION_IX2" ON "pebedba"."USERINFORMATION" ("IS_TEST_USER") ;
  CREATE INDEX "pebedba"."USERINFORMATION_IX3" ON "pebedba"."USERINFORMATION" ("LASTMODIFIED");
  